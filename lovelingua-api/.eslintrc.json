{
  "root": true,
  "env": {
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:import/recommended", "plugin:import/typescript", "prettier"],
  "rules": {
    "import/order": ["error", { "newlines-between": "always" }]
  }
}
